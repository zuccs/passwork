{
  "name": "Hide Passwork Junk",
  "version": "0.0.1",
  "manifest_version": 2,
  "description": "Super basic extension to hide junk on Passwork",
  "homepage_url": "https://daniel.zuccon.com",
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "default_locale": "en",
  "permissions": [
    "https://passwork.me/*"
  ],
  "content_scripts": [
    {
      "matches": [
        "https://passwork.me/*"
      ],
      "css": [
        "src/inject/inject.css"
      ]
    }
  ]
}